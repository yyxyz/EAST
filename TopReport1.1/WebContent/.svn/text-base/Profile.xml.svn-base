<?xml version="1.0" encoding="UTF-8"?>
<root>
	<!-- 获取菜单XML接口 -->
	<httpServiceURL></httpServiceURL>
	<endPoint>/login/menu_index.jsp</endPoint>
	<!-- 退出接口 -->
	<loginoutURL>/custlogout.do</loginoutURL>

<!-- 自定义控件属性设置 -->
<!-- 主页名称 -->
	<mainPageName>主页</mainPageName>
	<!-- 主菜单之间的间隔 -->
	<mainmenuGap>40</mainmenuGap>
	<!-- 主菜单边框颜色 -->
	<mainmenuborderColor>0x7798c6</mainmenuborderColor>
	<!-- 主菜单边框粗细 -->
	<mainmenuborderWeight>2</mainmenuborderWeight>
	<!-- 主菜单组件宽度 -->
	<mainmenuWidth>125</mainmenuWidth>
	<!-- 主菜单组件高度 -->
	<mainmenuHeight>125</mainmenuHeight>
	<!-- 主菜单底座颜色 -->
	<mainmenuBasebackgroundFill1>0x373737</mainmenuBasebackgroundFill1>
	<mainmenuBasebackgroundFill2>0x5A5A5A</mainmenuBasebackgroundFill2>
	<!-- 主菜单底座透明度 -->
	<mainmenuBaseAlphas>0.3</mainmenuBaseAlphas>
	<!-- 背景颜色 -->
	<backgroundFill1>0xACC0E3</backgroundFill1>
	<backgroundFill2>0xD9E9F8</backgroundFill2>
</root>
