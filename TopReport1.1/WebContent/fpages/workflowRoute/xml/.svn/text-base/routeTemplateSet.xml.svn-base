<?xml version="1.0" encoding="UTF-8"?>
<!--  用于直接生成SQL语句，支持同步和异步提交 -->
<!--  title:标题
	  navigate:导航信息
	  type:方式（sql|call)
	  interface:是否显示输入条件页面(true|false)
	  databusid:数据总线编号
	  transdataactionurl:数据整理URL
	  async:异步提交标志
	  pagesize:页面显示记录数(缺省为10)
-->
<CommQry
title="审核路线模板设置"
navigate="审核路线模板设置"
type="call"
interface="false"
pagesize="10"
async="true"
databusid="DEMO"
transdataactionurl="/trans/TransDataAction.do"
getterclassname="com.huateng.ebank.business.workflowRoute.getter.RouteTemplateQueryGetter"
>
	<Include id="BankParam"/>
	<ResultViewParam>
		/fpages/management/ftl/RouteTemplateSet.ftl
	</ResultViewParam>
	<PageQryExp type="CSV" limit="true" encoding="GBK" url="/trans/qryExp.do" directory="D:/templet"/>
	<Fields>
	<!-- status :F-基本信息中显示 N-不显示 D-详细信息中显示 A-基本、详细信息中都显示  -->
	<!-- primary : true-作为表单项记录上传　false-不作为表单项上传 -->

		<Field id="id" desc="审批路线编号" status="F" method="None" xpath="/id" datatype="long"   readonly="true"
			primary="true" type="routeId" size="20"  default="" tip="审批路线编号" />

		<Field id="routeName" desc="审批路线名称" status="N" method="None" xpath="/routeName" datatype="string"   readonly="false"
			primary="false" type="routeName" size="512"  default="." tip="审批路线编号"  require="true"/>
<!--
		<Field id="brhClass" desc="适用机构级别" status="N" method="None" xpath="/brhClass" translated="DATA_DIC.28" edittype="select"
			primary="true" type="brhClass" size="20" default="" tip="适用机构级别" require="true"/>
 -->
		<!-- modify by shen-antonio jira:BMS-2334 begin -->
		<Field id="brhClass" desc="机构递归关系" status="N" method="None" xpath="/brhClass"
			translated="DATA_DIC.28"
			edittype="select"   require="true"
			primary="false" type="brhClass" size="20" default="5" tip="机构递归关系" />
		<!-- modify by shen-antonio jira:BMS-2334 end -->

		<Field id="isBand" desc="是否绑定" status="F" method="None" xpath="/isBand" translated="DATA_DIC.8" edittype="select"
			primary="true" type="isBand" size="20" default="" tip="是否绑定" readonly="true"/>

		<Field id="status" desc="是否有效" status="F" method="None" xpath="/status" translated="DATA_DIC.8" edittype="select"
			primary="true" type="status" size="20" default="" tip="是否有效" readonly="true"/>

		<Field id="isSet" desc="是否已设置路线" status="F" method="None" xpath="/isSet" translated="DATA_DIC.8" edittype="select"
			primary="true" type="isSet" size="20" default="" tip="是否已设置路线" readonly="true"/>
		<!-- added by Henry 20110617  -->
		<Field id="opr" desc="操作" require="false" edittype="text"
			datatype="string" method="None" readonly="true" primary="true" type="操作"
			size="12" default="" xpath="" tip="操作" />
	</Fields>
	<Where>
		<!-- modify by shen-antonio jira:BMS-2334 begin -->
		 <Option id="brhClass" tablename="ci" fieldname="brhClass"
			operation="EQ"  size=""  desc="机构递归关系" tip="机构递归关系"
			type="string" edittype="select" translated="DATA_DIC.28" require=""
			viewField="procNameName"
			method="None"   index="1" width="" height="" default="" onchange=""
		    readOnly="">
		</Option>
		<!-- modify by shen-antonio jira:BMS-2334 end-->
	</Where>
	<Operations>
		<Button id="btNew" name="btNew" desc="增加新记录" operation="addrecord"
		url="" txn=""/>
		<Button id="btDelete" name="btDelete" desc="删除" operation="delrecord"
		url="" txn=""/>
		<!-- add by jornezhang 20100108 BMS-2362  增加交易流水记录 begin -->
		<!-- modify by shen-antonio jira:BMS-2334 begin -->
		<Button id="btSave" name="btSave" desc="保存" operation="asysubmit"
		updateClass="com.huateng.ebank.business.workflowRoute.updater.SaveRouteTemplateUpdate"
		url="#" txn="management.routetemplatesetsave.save"/>
		<!-- modify by shen-antonio jira:BMS-2334 end -->
		<!-- add by jornezhang 20100108 BMS-2362  增加交易流水记录 begin -->
		<Button id="btDetail" name="btDetail" desc="明细" operation="syncsubmit" targetFrame="_blank"
		url="/fpages/management/ftl/RouteDetailSet.ftl" txn=""/>


	</Operations>



</CommQry>
