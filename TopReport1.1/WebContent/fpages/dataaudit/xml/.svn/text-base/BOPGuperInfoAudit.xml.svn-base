	           size="" default="" xpath="/complianceno" />

		<Field id="remark" desc="备注" require="false" status="F" edittype="select" datatype="string"  size="30" xpath="/remark" />

	    <Field id="crtTm" desc="创建时间" require="" status="N" edittype="text" datatype="string"
	           size="" default="" xpath="/crtTm"  tip=""/>

		<Field id="brNo" desc="机构号" require=""  edittype="text" datatype="string" method="None"
 	  	       readonly="false" size="32" status="N" default="" xpath="/brNo" />

        <Field id="filler2" desc="业务流水号" require="" edittype="text" datatype="string" method="None"
	 	       readonly="false" size="" status="N" default="" xpath="/filler2"/>

		<Field id="brNoName" desc="机构名称" status="N"
			method="com.huateng.ebank.business.common.CQMethod.getBrnoName"
			width="" readonly="" primary="true" type="String" size="150"
			xpath="/brNo" default=""/>

        <Field id="approveStatusChoose" desc="审核状态选择" require="false" edittype="select" datatype="string" method="None"
	 	  readonly="false" size="2" translated="LIST:01,通过;02,不通过;" status="N" default="" xpath="" />

		<Field id="approveResultChoose" desc="审核说明" require="" edittype="textarea" datatype="string" method="None"
	 	  readonly="false" size="256" status="N" default="" xpath=""/>

	 	<Field id="actiontype" desc="操作类型" status="F" require="false" edittype="select" translated="DATA_DIC.10" size="1" datatype="string" default="" xpath="/actiontype" tip="操作类型" />

	 	<Field id="repStatus" desc="回执状态" status="N" require="false" edittype="select" size="2" translated="DATA_DIC.63" datatype="string" default="" xpath="/repStatus" tip="回执状态" />

	 	<Field id="approveStatus" desc="审核状态" status="N" require="false" edittype="select" size="2" translated="DATA_DIC.64" datatype="string" default="" xpath="/approveStatus" tip="审核状态" />

	 	<Field id="recStatus" desc="记录状态" status="N" require="false" edittype="select" size="2" translated="DATA_DIC.62" datatype="string" default="" xpath="/recStatus" tip="记录状态" />

	 	<Field id="pguperamount" desc="购汇履约金额" status="F" require="" edittype="text" rules="isRate_22_2"   datatype="currency" scale="2" size="22"   xpath="/pguperamount" />

		<Field id="gupercurr" desc="履约币种" status="F" require="" edittype="select" translated="CQ:CurrencySelect" dropdowntype="dynamic" method="None" viewField="currencyNoName"
                fieldmap="gupercurr=currencyNo;gupercurrName=currencyNoName" init="false" size="3" default="" xpath="/gupercurr" tip=""/>


         <Field id="gupercurrName" desc="履约币种" status="F" require="" method="com.huateng.ebank.business.common.CQMethod.getCurNameByCurNo" width="" readonly="" primary="true" type="String" size="150" init="false"
			xpath="/gupercurr" default=""/>
	    <Field id="guperdate" desc="履约日期" status="F" require="" edittype="text" datatype="date"
	           size="" default="" xpath="/guperdate"  tip=""/>

		<Field id="guperamount" desc="履约金额" status="F" require="" edittype="text" rules="isRate_22_2"   datatype="currency" scale="2" size="22" method="None"
	           default="" xpath="/guperamount"  tip=""/>

	    <Field id="bencode" desc="受益人代码" require="" edittype="text" datatype="string" method="None"
	 	       readonly="false" size="" status="F" default="" xpath="/bencode"/>

	 	<Field id="bename" desc="受益人中文名称" require="" edittype="text" datatype="string" method="None"
	 	       readonly="false" size="" status="F" default="" xpath="/bename"/>

	 	<Field id="benamen" desc="受益人英文名称" require="" edittype="text" datatype="string" method="None"
	 	       readonly="false" size="" status="F" default="" xpath="/benamen"/>
	</Fields>

	<Where>

		<DateTextBox id="qstartDate" desc="开始工作日期" tip="开始工作日期"
			require="false" status="F" datatype="date" method="None" size="150" width=""
			primary="false" index="0" type="String" />

		<DateTextBox id="qendDate" desc="结束工作日期" tip="结束工作日期"
			require="false" status="F" datatype="date" method="None" size="150" width=""
			primary="false" index="1" type="String" />
		<Option id="qActiontype" desc="操作类型" require="false" edittype="select" status="F"  translated="DATA_DIC.10" size="1" datatype="string"   index="2"/>

		<Option id="qRecStatus" desc="记录状态" require="false" edittype="select" size="2" status="N" translated="LIST:03,确认待审核;04,审核未确认;" datatype="string"   index="3"/>

		<Option id="qApproveStatus" desc="审核状态" require="false" edittype="select" size="2" status="N" translated="DATA_DIC.64" datatype="string" default="" xpath="/notCollectFlag" index="4"/>

		<Option id="qRepStatus" desc="回执状态" require="false" edittype="select" size="2" status="N" translated="DATA_DIC.63" datatype="string" default=""  index="5"/>

		<TextBox id="qFiller2" desc="业务流水号" tip="业务流水号" require="false" status="F" edittype="text"  datatype="string" size="28" method="None" primary="false" index="6"   />


    </Where>
	<Operations>
        <Button id="btAudit" name="btAudit" desc="审核" operation=""
			url="#" show="" group="" txn="" icon="icon-edit"/>
        <Button id="btAuditSave" name="btAuditSave" desc="确认" operation="asysubmit"
			updateClass="com.huateng.report.update.BOPGuperAuditUpdate" url="#" show="" group="" txn="" icon="icon-ok"/>
		<Button id="btBack" name="btBack" desc="取消" operation=""
			url="" show="" group="" txn="" icon="icon-cancel"/>

  	</Operations>
</CommQry>
