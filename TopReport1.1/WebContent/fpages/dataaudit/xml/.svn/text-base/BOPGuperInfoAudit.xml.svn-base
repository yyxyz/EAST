<?xml version="1.0" encoding="UTF-8"?>
<CommQry
title="对外担保信息"
navigate="主页 &gt; 对外担保信息 &gt; 责任余额变动信息"
type="call"
interface="true"
pagesize="10"
async="true"
databusid=""
transdataactionurl="/trans/TransDataAction.do"
getterclassname="com.huateng.report.getter.BOPGuperInfoAuditGetter">
	<Include id="BankParam" />

	<Fields>

		 <!-- BopCfaExguDs 对外担保信息表-->
		 <Field id="select"  desc="选择" method="None" edittype="checkbox" datatype="boolean" xpath="/select"/>

       <Field id="appdocuno" desc="核准文件号" status="N" require="false" edittype="text" datatype="string" readonly=""
              size="" default="" xpath="/appdocuno" tip="" />

	    <Field id="guarantorcode" desc="担保人代码" require="false" edittype="text" datatype="string" method="None"
	           size="" default="" status="F" xpath="/guarantorcode"  tip=""/>

	    <Field id="buscode" desc="银行业务参号" require="false" edittype="text" datatype="string"
	           size="" default="" status="F" xpath="/buscode" tip=""/>

	    <Field id="workDate" desc="工作日期" require="false" edittype="text" datatype="string"
	           size="" default="" status="N" xpath="/workDate"  tip=""/>

		<Field id="exguarancode" desc="对外担保编号" require="" edittype="text" datatype="string" method="None" readonly=""
	 		   size=""  status="F" default="" xpath="/exguarancode" />

	 	<Field id="guaranamount" desc="保函金额" require="" edittype="text" datatype="string" method="None" readonly=""
	 		   size=""  status="N" default="" xpath="/guaranamount" />

		<Field id="lstUpdTm" desc="最后更新时间" require="false"  edittype="text" datatype="date" method="None"
	           size="" default="" status="N"  xpath="/lstUpdTm" />

		<Field id="lstUpdTlr" desc="最后更新人" status="N" require="false"  edittype="text" datatype="string"  size="" xpath="/lstUpdTlr" />

		<Field id="contractdate" desc="签约日期" status="N" require="false" edittype="text" datatype="string" method="None"
	           size="" default="" xpath="/contractdate" tip=""/>

	    <Field id="guarantype" desc="担保类型"  status="N"  edittype="text" datatype="string"
	           size="" default="" xpath="/guarantype"  tip=""/>

	    <Field id="maturity" desc="到期日" require="false" status="N" edittype="select" datatype="string"
	           size="" default="" xpath="/maturity"  tip=""/>

	    <Field id="wabachandate" desc="担保责任余额变动日期" status="N" require=""  edittype="text" datatype="string" method="None"
