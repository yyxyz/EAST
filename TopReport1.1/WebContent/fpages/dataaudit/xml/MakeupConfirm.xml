<?xml version="1.0" encoding="UTF-8"?>

<!--  用于直接生成SQL语句，支持同步和异步提交 -->
<!--  title:标题
	navigate:导航信息
	type:方式（sql|call)
	interface:是否显示输入条件页面(true|false)
	databusid:数据总线编号
	transdataactionurl:数据整理URL
	txnlogflag:true记录日志
	async:异步提交标志
	pagesize:页面显示记录数(缺省为10)
-->

<!--approveStatus:审核状态
	    00-未审核
        01-通过
        02-不通过
        03-拒绝

	recStatus:记录状态
	    01-可编辑
        02-编辑待确认
        03-确认待审核
        04-审核未确认
        05-审核已确认
        06-已上报
	actiontype:操作类型
	    A 创建
        C 修改
        D 删除
	repStatus:回执状态
	    00-未返回
        01-回执成功
        02-回执失败
-->


<CommQry title="补录确认" navigate="上报数据补录 > 补录完成确认" type="call"
	interface="false" pagesize="1000" async="true" databusid=""
	transdataactionurl="/trans/TransDataAction.do"
	txnlogflag="true"
	getterclassname="com.huateng.report.getter.MakeupConfirmGetter">
	<Include id="BankParam" />
	<PageQryExp type="XLS" limit="true" encoding="GBK" url="/trans/qryExp.do" desc="数据导出-补录完成确认" perfetch="100" />

	<Fields>
		<!-- status :F-基本信息中显示  N-不显示 D-详细信息中显示 A-基本、详细信息中都显示  -->
		<!-- primary : true-作为表单项记录上传　false-不作为表单项上传 -->
		<Field id="workDate" desc="工作日期" edittype="text" datatype="string" require="" xpath="/workDate"  status="F"/>
		<Field id="apptype" desc="应用类型" require="" edittype="select" datatype="string" method="None"
		  readonly="false" size="20" status="F" default=""  translated="DATA_DIC.17" primary="true" xpath="/apptype" />

		<Field id="currentfile" desc="文件类型代码" require="" edittype="text" datatype="string"
		          status="F" default=""  xpath="/currentfile" />

		<Field id="busiType" desc="业务类型" require="" edittype="text" datatype="string"
		           default=""  xpath="/busiType" />

		<Field id="fileType" desc="文件类型" require="" edittype="text" datatype="string" method="com.huateng.ebank.business.common.CQMethod.getCurrentFile"
		  readonly="false" size="20" status="F" default="" primary="true" xpath="/apptype|'_'|/currentfile" />

		<Field id="hashNum" desc="补录已处理" edittype="text" datatype="int" xpath="/hashNum" status="F"/>

		<Field id="noNum" desc="补录未处理" edittype="text" datatype="int" xpath="/noNum" status="F"/>

		<Field id="opr" desc="操作" status="N" method="None" xpath=""
			primary="true" type="" size="20" default="" tip="操作" require=""/>


       </Fields>

	<Parameters></Parameters>
	<Where>
		 <TextBox id="workDateStart" desc="开始工作日期" datatype="date" require="false" index="0" readonly="false"/>
		 
		 <TextBox id="workDateEnd" desc="结束工作日期" datatype="date" require="false" index="1" readonly="false"/>

		 <Option id="busiType" desc="业务类型" datatype="string" require="true" index="2" translated="DATA_DIC.35"></Option>
		 
		 <Option id="qappType" operationb="EQ" size="" index="3"
        	desc="应用类型" tip="应用类型" type="string" translated="CQ:AppTypeSelect"
        	fieldmap="qappTypeName=dataName;qappType=dataNo" viewField="dataName"
        	ropdowntype="dynamic" init="true" require="true" readOnly=""/>

		 <Option id="isShowZero" desc="是否显示补录为0的数据" datatype="string" require="false" index="4" translated="DATA_DIC.10004" readonly="false" dropdowntype="radio"/>
	</Where>

	<Operations>
	    <Button id="btConfirm" name="btConfirm" desc="补录完成确认" operation="asysubmit" updateClass="com.huateng.report.update.MakeupConfirmUpdate" url="#" icon="icon-ok"/>

		<Button id="btDetail" name="btDetail" desc="详细"   url="#"  operation="syncsubmit" show="" group="" txn="" ></Button>
	</Operations>

	<Script>
	</Script>

</CommQry>