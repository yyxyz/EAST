<?xml version="1.0" encoding="UTF-8"?>

<!-- 用于直接生成SQL语句，支持同步和异步提交 -->
<!-- title:标题 navigate:导航信息 type:方式（sql|call) interface:是否显示输入条件页面(true|false) 
	databusid:数据总线编号 transdataactionurl:数据整理URL txnlogflag:true记录日志 async:异步提交标志 
	pagesize:页面显示记录数(缺省为10) -->
<CommQry title="导入文件配置" navigate="导入文件配置" type="call" interface="false"
	pagesize="7" async="true" databusid="" transdataactionurl="/trans/TransDataAction.do"
	txnlogflag="true" getterclassname="com.huateng.report.imports.getter.ImportConfigFileGetter">
	<Include id="BankParam" />
	<!-- <PageQryExp type="CSV" limit="true" encoding="GBK" url="/trans/qryExp.do" 
		directory="F:/upload"/> -->
	<Fields>
		<!-- status :F-基本信息中显示 N-不显示 D-详细信息中显示 A-基本、详细信息中都显示 -->
		<!-- primary : true-作为表单项记录上传 false-不作为表单项上传 -->

		<Field id="id" desc="" edittype="text" datatype="String" size="32"
			require="true" xpath="/id" />
		<Field id="fileName" desc="文件名称" edittype="text" status="F"
			datatype="string" require="true" size="20" xpath="/fileName" />
		<Field id="tableName" desc="表名" edittype="text" status="F"
			datatype="string" require="true" size="20" xpath="/tableName" />
		<Field id="batchNo" desc="批次" edittype="text" datatype="int"
			status="F" require="" xpath="/batchNo" />
		<Field id="fileType" desc="文件类型" edittype="select" require="true" translated="DATA_DIC.10001" datatype="string" xpath="/fileType" />
		<Field id="splitType" desc="字段分割类型" edittype="select"
			translated="DATA_DIC.10002" datatype="string"
			xpath="/splitType" />
		<Field id="seperator" desc="分隔符" edittype="text" datatype="string"
			size="5" xpath="/seperator" />
		<Field id="importTime" desc="导入时机" edittype="select"
			translated="DATA_DIC.10005" datatype="string" 
			xpath="/importTime" />
		<Field id="seqNo" desc="导入顺序号" edittype="text" datatype="int"
			xpath="seqNo" />
		<Field id="updateType" desc="更新方式" require="true" edittype="select"
			translated="DATA_DIC.10003"
			datatype="string" xpath="updateType" />
		<Field id="startRow" desc="起始行" edittype="text" require="true" datatype="int"
			xpath="/startRow" />
		<Field id="endrowFlag" desc="是否读结束行" require="true" edittype="select"
			translated="DATA_DIC.10004" datatype="string" xpath="/endrowFlag" />
		<Field id="startColumn" desc="起始列(仅用于Excel)" edittype="text"
			datatype="int" xpath="/startColumn" />
		<Field id="endColumn" desc="结束列(仅用于Excel)" edittype="text"
			datatype="int" xpath="/endColumn" />
		<Field id="sheetNum" desc="Sheet页(仅用于Excel)" edittype="text"
			datatype="int" xpath="/sheetNum" />
		<Field id="mainFlag" desc="主文件标志" edittype="text" 
			datatype="string" xpath="/mainFlag" />
		<Field id="fuid" desc="主文件ID" edittype="text" size="16"
			datatype="string" xpath="/fuid" />
		<Field id="status" desc="是否生效标志" edittype="select" require="true" translated="DATA_DIC.10004"
			datatype="string" xpath="/status" />
		<Field id="remark" desc="备注" edittype="text" datatype="string"
			size="100" xpath="/remark" />

	</Fields>

	<Parameters></Parameters>


	<Operations>
		<Button id="btNew" name="btNew" desc="新增" operation="addrecord"
			url="#" show="" group="" txn="" icon="icon-add"/>
		<Button id="btMod" name="btMod" desc="修改" operation="" url="#"
			show="" group="" txn="" icon="icon-edit"/>
		<Button id="btDel" name="btDel" desc="删除" operation="delrecord"
			url="#" txn="" icon="icon-remove"/>
		<Button id="btSave" name="btSave" desc="保存" operation="asysubmit"
			url="#" updateClass="com.huateng.report.imports.updater.ImportConfigFileUpdate" icon="icon-save"/>
		<Button id="Cancel" name="btCancel" desc="取消"  operation="" url="#"  show="" group="" txn="" icon="icon-cancel"/>
		<Button id="btRow" name="btRow" desc="导入字段配置" operation="syncsubmit"
			url="" icon="icon-set"/>
		<Button id="iXml" name="btRow" desc="XML导入配置" operation="syncsubmit"
			url="" icon="icon-set"/>
	</Operations>

	<Script></Script>

</CommQry>