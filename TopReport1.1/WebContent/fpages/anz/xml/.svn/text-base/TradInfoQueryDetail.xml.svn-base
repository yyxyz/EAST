<?xml version="1.0" encoding="UTF-8"?>
<!--  用于直接生成SQL语句，支持同步和异步提交 -->
<!--  title:标题
	navigate:导航信息
	type:方式（sql|call)
	interface:是否显示输入条件页面(true|false)
	databusid:数据总线编号
	transdataactionurl:数据整理URL
	txnlogflag:true记录日志
	async:异步提交标志
	pagesize:页面显示记录数(缺省为10)
-->

<CommQry title="交易信息查询明细" navigate="澳新银行&gt;交易信息明细查询"
    type="call" interface = "true" pagesize="15" async="true" databusid=""
    transdataactionurl="/trans/TransDataAction.do" txnlogflag="true"
    getterclassname="com.huateng.report.anz.getter.TradInfoQueryDetailGetter" >
<Include id="BankParam" />

<PageQryExp type="XLS" limit="true" encoding="GBK" url="/trans/qryExp.do" desc="交易信息明细查询"/>
<Fields>
		<!-- status :F-基本信息中显示  N-不显示 D-详细信息中显示 A-基本、详细信息中都显示  -->
		<!-- primary : true-作为表单项记录上传　false-不作为表单项上传 -->

		<Field id="updt" desc="日期" edittype="text" require="false" size="20" errmsg=""  datatype="string" status="F" xpath="/updt"  />
		<Field id="cnum" desc="客户账号" edittype="text" size="30" require="false" errmsg="" datatype="string" status="F" xpath="/cnum" />
		<Field id="inamt" desc="汇总入账" edittype="text" size="10" require="false" errmsg="" datatype="currency"  scale="2" status="F" xpath="/inamt" />
		<Field id="outamt" desc="汇总出账" edittype="text" size="20" require="false" errmsg="" datatype="currency" scale="2" status="F" xpath="/outamt" />
		<Field id="cmrm" desc="客户经理号" edittype="text" size="20" require="false" errmsg="" datatype="string" status="F" xpath="/cmrm" />
		<Field id="solId" desc="机构号" edittype="text" size="20" require="false" errmsg="" datatype="string" status="F" xpath="/solId" />
		
</Fields>

<Where >
	<TextBox id="Qid" dataType="date" size="" desc="交易日期"
			tip="交易日期" index="0" default="" scale="" require="" readOnly=""
			rules="" errmsg="" />
</Where>

<Operations>
	<Button id="btNew" name="btNew" desc ="新增" operation="addrecord" url="#" show="" group="" txn="" />
	<Button id="btMod" name="btMod" desc="修改" operation="syncsubmit" url="#" show="" group="" txn="" />

	<Button id="btDel" name="btDel" desc="删除"
	         operation="asysubmit"
	         updateClass="com.huateng.report.anz.updater.ProductsInfoDelUpdate"
	         url="#"  txn="" />

	<Button id="btSave" name="btSave" desc="保存"
			operation="asysubmit"
			updateClass="com.huateng.report.anz.updater.ProductsInfoUpdate"
			url="#" txn="" />

</Operations>
	<Script></Script>
 </CommQry>