			translated="DATA_DIC.8000"  datatype="string"  size="1" default="" xpath="/spapfeboindex" tip="是否经外汇局特批不需占用指标" />

		<Field id="remark" desc="备注" require="" edittype="textarea" datatype="string" method="None"
