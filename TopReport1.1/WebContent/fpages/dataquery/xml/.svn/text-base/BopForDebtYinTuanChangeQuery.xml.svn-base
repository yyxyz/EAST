	 	  readonly="false" size="32" status="F" default="" xpath="/filler2" />

		<Field id="filler3" desc="预留字段3" require="" edittype="text" datatype="string" method="None"
	 	  readonly="false" size="32" status="N" default="" xpath="/filler3" />

