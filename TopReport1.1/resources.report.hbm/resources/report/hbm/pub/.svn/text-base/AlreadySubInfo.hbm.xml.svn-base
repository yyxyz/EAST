<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="AlreadySubInfo"
		table="ALREADY_SUB_INFO"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="ID"
		>
		</id>

		<property
			name="apptype"
			column="APPTYPE"
			type="string"
			not-null="true"
			length="3"
		/>
		<property
			name="currentfile"
			column="CURRENTFILE"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="recId"
			column="REC_ID"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="bussno"
			column="BUSSNO"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="recDate"
			column="REC_DATE"
			type="string"
			not-null="false"
			length="6"
		/>
		<property
			name="brNo"
			column="BR_NO"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="fileName"
			column="FILE_NAME"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="fileSeq"
			column="FILE_SEQ"
			type="string"
			not-null="false"
			length="2"
		/>


	</class>	
</hibernate-mapping>