<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="CustomerInfo"
		table="customer_info"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="cnum"
		>
		</id>

		<property
			name="createdate"
			column="createdate"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="crum"
			column="crum"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="chineseName"
			column="chinese_name"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="idno"
			column="idno"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="birthday"
			column="birthday"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="solId"
			column="sol_id"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="cloc"
			column="cloc"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="homePhoneNumber"
			column="home_phone_number"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="mobilePhoneNumber"
			column="mobile_phone_number"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="zipCode"
			column="zip_code"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="address"
			column="address"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="acrm"
			column="acrm"
			type="string"
			not-null="false"
			length="12"
		/>
		<property
			name="cmrm"
			column="cmrm"
			type="string"
			not-null="false"
			length="12"
		/>
		<property
			name="vipflg"
			column="vipflg"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="vipClass"
			column="vip_class"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="updt"
			column="updt"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="lastUpdtVip"
			column="last_updt_vip"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="lastVipflg"
			column="last_vipflg"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="lastUpdtAcrm"
			column="last_updt_acrm"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="closeflg"
			column="closeflg"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="closedt"
			column="closedt"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="miscflg"
			column="miscflg"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="desc1"
			column="desc1"
			type="string"
			not-null="false"
			length="254"
		/>
		<property
			name="desc2"
			column="desc2"
			type="string"
			not-null="false"
			length="254"
		/>


	</class>	
</hibernate-mapping>