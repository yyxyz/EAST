<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="MtsBopInvcountrycode"
		table="MTS_BOP_INVCOUNTRYCODE"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="INVCOUNTRYCODE_ID"
		>
		</id>

		<property
			name="invcountrycode"
			column="INVCOUNTRYCODE"
			type="string"
			not-null="false"
			length="3"
		/>

		<property
			name="recId"
			column="REC_ID"
			type="string"
			length="32"
		/>
	</class>
</hibernate-mapping>