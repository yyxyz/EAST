<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="MtsBopRefnos"
		table="MTS_BOP_REFNOS"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="ID"
		>
		</id>

		<property
			name="refno"
			column="REFNO"
			type="string"
			not-null="false"
			length="9"
		/>
		<property
			name="recId"
			column="REC_ID"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="crtTm"
			column="CRT_TM"
			type="java.util.Date"
			not-null="false"
			length="34"
		/>


	</class>	
</hibernate-mapping>