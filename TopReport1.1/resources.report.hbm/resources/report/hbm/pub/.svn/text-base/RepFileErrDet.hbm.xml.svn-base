<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="RepFileErrDet"
		table="REP_FILE_ERR_DET"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="DET_ID"
		>
			<generator class="uuid" />
		</id>

		<property
			name="repFileName"
			column="REP_FILE_NAME"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="apptype"
			column="APPTYPE"
			type="string"
			not-null="false"
			length="3"
		/>
		<property
			name="currentfile"
			column="CURRENTFILE"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="repErrType"
			column="REP_ERR_TYPE"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="errdesc"
			column="ERRDESC"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="bussno"
			column="BUSSNO"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="errfield"
			column="ERRFIELD"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="errfieldcn"
			column="ERRFIELDCN"
			type="string"
			not-null="false"
			length="128"
		/>


	</class>	
</hibernate-mapping>