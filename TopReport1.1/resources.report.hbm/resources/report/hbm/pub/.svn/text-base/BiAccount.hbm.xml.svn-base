<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="BiAccount"
		table="BI_ACCOUNT"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="id"
			type="string"
			column="ACCOUNT_ID"
		>
		</id>

		<property
			name="accountCur"
			column="ACCOUNT_CUR"
			type="string"
			not-null="false"
			length="3"
		/>
		<property
			name="customerId"
			column="CUSTOMER_ID"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="accountType"
			column="ACCOUNT_TYPE"
			type="string"
			not-null="false"
			length="4"
		/>
		<property
			name="accountProperty"
			column="ACCOUNT_PROPERTY"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="openTime"
			column="OPEN_TIME"
			type="string"
			not-null="false"
			length="16"
		/>
		<property
			name="closeTime"
			column="CLOSE_TIME"
			type="string"
			not-null="false"
			length="16"
		/>
		<property
			name="abroadFlag"
			column="ABROAD_FLAG"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="enCode"
			column="EN_CODE"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="chineseName"
			column="CHINESE_NAME"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="fileNumber"
			column="FILE_NUMBER"
			type="string"
			not-null="false"
			length="28"
		/>
		<property
			name="customerIdExt"
			column="CUSTOMER_ID_EXT"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="debits"
			column="DEBITS"
			type="big_decimal"
			not-null="false"
			length="22"
		/>
		<property
			name="credits"
			column="CREDITS"
			type="big_decimal"
			not-null="false"
			length="22"
		/>
		<property
			name="balance"
			column="BALANCE"
			type="big_decimal"
			not-null="false"
			length="22"
		/>
		<property
			name="dealDate"
			column="DEAL_DATE"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="isnraFlag"
			column="ISNRA_FLAG"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="filler1"
			column="FILLER1"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="filler2"
			column="FILLER2"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="filler3"
			column="FILLER3"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="accountStatus"
			column="ACCOUNT_STATUS"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="amtype"
			column="AMTYPE"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="accAccountType"
			column="ACC_ACCOUNT_TYPE"
			type="string"
			not-null="false"
			length="4"
		/>
		<property
			name="accountCata"
			column="ACCOUNT_CATA"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="limitType"
			column="LIMIT_TYPE"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="accountLimit"
			column="ACCOUNT_LIMIT"
			type="big_decimal"
			not-null="false"
			length="22"
		/>


	</class>	
</hibernate-mapping>