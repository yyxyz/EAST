<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="BopProjectInfo"
		table="BOP_PROJECT_INFO"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="PROJ_ID"
		>
		</id>

		<property
			name="projectname"
			column="PROJECTNAME"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="recId"
			column="REC_ID"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="crtTm"
			column="CRT_TM"
			type="java.util.Date"
			not-null="false"
			length="14"
		/>


	</class>	
</hibernate-mapping>