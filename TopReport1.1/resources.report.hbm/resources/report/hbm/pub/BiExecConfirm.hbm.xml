<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="BiExecConfirm"
		table="BI_EXEC_CONFIRM"
	>
		<meta attribute="sync-DAO">false</meta>
		<composite-id name="id" class="BiExecConfirmPK">
			<key-property
				name="busiType"
				column="BUSI_TYPE"
				type="string"
			/>
			<key-property
				name="brNo"
				column="BR_NO"
				type="string"
			/>
			<key-property
				name="workDate"
				column="WORK_DATE"
				type="string"
			/>
		</composite-id>

		<property
			name="confirmStatus"
			column="CONFIRM_STATUS"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="subfileStatus"
			column="SUBFILE_STATUS"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="confirmTlrNo"
			column="CONFIRM_TLR_NO"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="confirmTm"
			column="CONFIRM_TM"
			type="java.util.Date"
			not-null="false"
			length="34"
		/>
		<property
			name="subfileTlrNo"
			column="SUBFILE_TLR_NO"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="subfileTm"
			column="SUBFILE_TM"
			type="java.util.Date"
			not-null="false"
			length="34"
		/>
		<property
			name="confirmRemark"
			column="CONFIRM_REMARK"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="subfileRemark"
			column="SUBFILE_REMARK"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="lstUpdTm"
			column="LST_UPD_TM"
			type="java.util.Date"
			not-null="false"
			length="34"
		/>
		<property
			name="lstUpdTlr"
			column="LST_UPD_TLR"
			type="string"
			not-null="false"
			length="20"
		/>


	</class>	
</hibernate-mapping>