<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="BiProcessLog"
		table="BI_PROCESS_LOG"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="PRO_ID"
		>
		</id>

		<property
			name="workDate"
			column="WORK_DATE"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="busiType"
			column="BUSI_TYPE"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="brNo"
			column="BR_NO"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="executeType"
			column="EXECUTE_TYPE"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="executeTm"
			column="EXECUTE_TM"
			type="java.util.Date"
			not-null="false"
			length="11"
		/>
		<property
			name="executeId"
			column="EXECUTE_ID"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="startTm"
			column="START_TM"
			type="java.util.Date"
			not-null="false"
			length="11"
		/>
		<property
			name="endTm"
			column="END_TM"
			type="java.util.Date"
			not-null="false"
			length="11"
		/>
		<property
			name="operType"
			column="OPER_TYPE"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="ip"
			column="IP"
			type="string"
			not-null="false"
			length="32"
		/>

	</class>	
</hibernate-mapping>