<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="BiAreaOfChina"
		table="BI_AREAOFCHINA"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="areacode"
			type="string"
			column="AREA_CODE"
		>
		</id>

		<property
			name="areaname"
			column="AREA_NAME"
			type="string"
			length="128"
		/>
		<property
			name="filler1"
			column="FILLER1"
			type="string"
			length="128"
		/>
		<property
			name="filler2"
			column="FILLER2"
			type="string"
			length="128"
		/>
		<property
			name="filler3"
			column="FILLER3"
			type="string"
			length="128"
		/>
		<property
			name="st"
			column="ST"
			type="string"
			length="1"
		/>
		<property
			name="lock"
			column="IS_LOCK"
			type="true_false"
			length="1"
		/>
		<property
			name="del"
			column="IS_DEL"
			type="true_false"
			length="1"
		/>
		<property
			name="crtDt"
			column="CRT_DT"
			type="string"
			length="8"
		/>
		<property
			name="lastUpdOper"
			column="LAST_UPD_OPER"
			type="string"
			length="20"
		/>
		<property
			name="lastUpdTms"
			column="LAST_UPD_TMS"
			type="string"
			length="14"
		/>

	</class>
</hibernate-mapping>