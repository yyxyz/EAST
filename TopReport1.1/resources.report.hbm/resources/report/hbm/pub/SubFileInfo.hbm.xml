<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="SubFileInfo"
		table="SUB_FILE_INFO"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="FILE_NAME"
		>
		</id>

		<property
			name="apptype"
			column="APPTYPE"
			type="string"
			not-null="true"
			length="3"
		/>
		<property
			name="currentfile"
			column="CURRENTFILE"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="brNo"
			column="BR_NO"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="crtTm"
			column="CRT_TM"
			type="java.util.Date"
			not-null="false"
			length="11"
		/>
		<property
			name="repFileName"
			column="REP_FILE_NAME"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="repTm"
			column="REP_TM"
			type="java.util.Date"
			not-null="false"
			length="11"
		/>
		<property
			name="filePack"
			column="FILE_PACK"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="repErrType"
			column="REP_ERR_TYPE"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="workdate"
			column="WORKDATE"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="isImpRep"
			column="IS_IMP_REP"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="totalrecords"
			column="TOTALRECORDS"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="sucrecords"
			column="SUCRECORDS"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="falrecords"
			column="FALRECORDS"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="subFileSeq"
			column="SUB_FILE_SEQ"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="fileDate"
			column="FILE_DATE"
			type="string"
			not-null="true"
			length="6"
		/>
		<property
			name="busiType"
			column="BUSI_TYPE"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="isSub"
			column="IS_SUB"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="subTm"
			column="SUB_TM"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="subType"
			column="SUB_TYPE"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="subTlr"
			column="SUB_TLR"
			type="string"
			not-null="false"
			length="20"
		/>

	</class>
</hibernate-mapping>