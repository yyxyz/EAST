<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="RbsBranchCodeMapp"
		table="RBS_BRANCH_CODE_MAPP"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="ID"
		>
		</id>

		<property
			name="rbsbranchcode"
			column="RBSBRANCHCODE"
			type="string"
			not-null="false"
			length="12"
		/>

		<property
			name="busitype"
			column="BUSI_TYPE"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="branchcode"
			column="BRANCHCODE"
			type="string"
			not-null="false"
			length="12"
		/>
		<property
			name="status"
			column="STATUS"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="crtTm"
			column="CRT_TM"
			type="java.util.Date"
			not-null="false"
			length="14"
		/>
		<property
			name="crtTlr"
			column="CRT_TLR"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="lstUpdTm"
			column="LST_UPD_TM"
			type="java.util.Date"
			not-null="false"
		/>
		<property
			name="lstUpdTlr"
			column="LST_UPD_TLR"
			type="string"
			not-null="false"
			length="20"
		/>


	</class>
</hibernate-mapping>