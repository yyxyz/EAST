<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="SysTaskConfig"
		table="SYS_TASK_CONFIG"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="TASKNO"
		>
		</id>

		<property
			name="taskname"
			column="TASKNAME"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="flag"
			column="FLAG"
			type="string"
			not-null="true"
			length="1"
		/>


	</class>	
</hibernate-mapping>