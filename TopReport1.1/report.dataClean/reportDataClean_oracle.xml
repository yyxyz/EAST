<?xml version="1.0" encoding="UTF-8"?>
<TABLES>
	<TABLE>
		<DISPLAYNAME>已报送文件</DISPLAYNAME>
		<NAME>SUB_FILE_INFO</NAME>
		<RESERVES_DAY>90</RESERVES_DAY>
		<DATE_FORMATE>yyMMdd</DATE_FORMATE>
		<DETS>
			<DET>
				<DET_DISPLAYNAME>已报送文件明细</DET_DISPLAYNAME>
				<DET_NAME>ALREADY_SUB_INFO</DET_NAME>
				<DET_DELSQL>delete from ALREADY_SUB_INFO where FILE_NAME in (select FILE_NAME from SUB_FILE_INFO where IS_SUB='Y' and IS_IMP_REP='Y' and FILE_DATE&lt;{0})</DET_DELSQL>
			</DET>
			<DET>
				<DET_DISPLAYNAME>回执错误信息</DET_DISPLAYNAME>
				<DET_NAME>REP_FILE_ERR_DET</DET_NAME>
				<DET_DELSQL>delete from REP_FILE_ERR_DET where REP_FILE_NAME in (select REP_FILE_NAME from SUB_FILE_INFO where REP_FILE_NAME is not null and IS_SUB='Y' and IS_IMP_REP='Y' and FILE_DATE&lt;{0})</DET_DELSQL>
			</DET>
		</DETS>
		<DEL_SQL>delete from SUB_FILE_INFO where IS_SUB='Y' and IS_IMP_REP='Y' and FILE_DATE&lt;{0}</DEL_SQL>
	</TABLE>
