<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="BiAnalyConf"
		table="BI_ANALY_CONF"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="CONF_ID"
		>
		</id>

		<property
			name="confSeq"
			column="CONF_SEQ"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="confClassPath"
			column="CONF_CLASS_PATH"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="confInfo"
			column="CONF_INFO"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="busiType"
			column="BUSI_TYPE"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="confDate"
			column="CONF_DATE"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="confVaild"
			column="CONF_VAILD"
			type="string"
			not-null="false"
			length="1"
		/>


	</class>	
</hibernate-mapping>