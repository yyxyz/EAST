<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="BiAnalyProcess"
		table="BI_ANALY_PROCESS"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="ANALY_NO"
		>
		</id>

		<property
			name="brNo"
			column="BR_NO"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="workDate"
			column="WORK_DATE"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="busiType"
			column="BUSI_TYPE"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="operTm"
			column="OPER_TM"
			type="java.util.Date"
			not-null="false"
			length="11"
		/>
		<property
			name="operTlr"
			column="OPER_TLR"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="executeTm"
			column="EXECUTE_TM"
			type="java.util.Date"
			not-null="false"
			length="11"
		/>
		<property
			name="appType"
			column="APP_TYPE"
			type="string"
			not-null="true"
			length="10"
		/>

	</class>
</hibernate-mapping>