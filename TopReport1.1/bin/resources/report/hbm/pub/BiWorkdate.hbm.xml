<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="BiWorkdate"
		table="BI_WORKDATE"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="WORK_DATE"
		>
		</id>

		<property
			name="workFlag"
			column="WORK_FLAG"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="operatorId"
			column="OPERATOR_ID"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="filler1"
			column="FILLER1"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="filler2"
			column="FILLER2"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="filler3"
			column="FILLER3"
			type="string"
			not-null="false"
			length="64"
		/>


	</class>	
</hibernate-mapping>