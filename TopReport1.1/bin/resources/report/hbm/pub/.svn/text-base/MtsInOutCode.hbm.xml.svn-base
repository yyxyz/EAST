<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="MtsInOutCode"
		table="MTS_IN_OUT_CODE"
	>
		<meta attribute="sync-DAO">false</meta>
		<composite-id name="id" class="MtsInOutCodePK">
			<key-property
				name="id"
				column="ID"
				type="string"
			/>
			<key-property
				name="codeType"
				column="CODE_TYPE"
				type="string"
			/>
		</composite-id>

		<property
			name="parentId"
			column="PARENT_ID"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="codeName"
			column="CODE_NAME"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="filler1"
			column="FILLER1"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="filler2"
			column="FILLER2"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="filler3"
			column="FILLER3"
			type="string"
			not-null="false"
			length="64"
		/>


	</class>	
</hibernate-mapping>