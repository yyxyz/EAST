<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="RbsDsBiBalance"
		table="RBS_DS_BI_BALANCE"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="string"
			column="ID"
		>
		</id>

		<property
			name="branchcode"
			column="BRANCHCODE"
			type="string"
			not-null="true"
			length="3"
		/>
		<property
			name="partynumber"
			column="PARTYNUMBER"
			type="string"
			not-null="true"
			length="12"
		/>
		<property
			name="currencycode"
			column="CURRENCYCODE"
			type="string"
			not-null="true"
			length="3"
		/>
		<property
			name="totaldtbal"
			column="TOTALDTBAL"
			type="big_decimal"
			not-null="false"
			length="22"
		/>
		<property
			name="totalcrbal"
			column="TOTALCRBAL"
			type="big_decimal"
			not-null="false"
			length="22"
		/>
		<property
			name="valuebal"
			column="VALUEBAL"
			type="big_decimal"
			not-null="true"
			length="22"
		/>
		<property
			name="dealdate"
			column="DEALDATE"
			type="string"
			not-null="true"
			length="14"
		/>
	</class>
</hibernate-mapping>