<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="BiAnalyDetailPars"
		table="BI_ANALY_DETAIL_PARS"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="PAR_ID"
		>
		</id>

		<property
			name="detId"
			column="DET_ID"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="parSeq"
			column="PAR_SEQ"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="parName"
			column="PAR_NAME"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="parValue"
			column="PAR_VALUE"
			type="string"
			not-null="false"
			length="256"
		/>


	</class>	
</hibernate-mapping>