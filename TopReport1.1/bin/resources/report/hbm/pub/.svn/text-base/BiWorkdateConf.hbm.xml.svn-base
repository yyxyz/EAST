<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="BiWorkdateConf"
		table="BI_WORKDATE_CONF"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="WORKDATE_YEAR"
		>
		</id>

		<property
			name="st"
			column="ST"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="lock"
			column="IS_LOCK"
			type="true_false"
			not-null="false"
			length="1"
		/>
		<property
			name="del"
			column="IS_DEL"
			type="true_false"
			not-null="false"
			length="1"
		/>
		<property
			name="crtDt"
			column="CRT_DT"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="lastUpdOper"
			column="LAST_UPD_OPER"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="lastUpdTms"
			column="LAST_UPD_TMS"
			type="string"
			not-null="false"
			length="14"
		/>


	</class>	
</hibernate-mapping>