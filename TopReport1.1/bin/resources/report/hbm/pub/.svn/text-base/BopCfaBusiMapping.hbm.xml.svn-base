<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="BopCfaBusiMapping"
		table="BOP_CFA_BUSI_MAPPING"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="FILE_TYPE"
		>
		</id>

		<property
			name="busiCode"
			column="BUSI_CODE"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="otherMapping"
			column="OTHER_MAPPING"
			type="string"
			not-null="false"
			length="512"
		/>
		<property
			name="exp1"
			column="EXP_1"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="exp2"
			column="EXP_2"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="exp3"
			column="EXP_3"
			type="string"
			not-null="false"
			length="256"
		/>


	</class>	
</hibernate-mapping>