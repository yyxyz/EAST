<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="BiBopjshRetNo"
		table="BI_BOPJSH_RET_NO"
	>
		<meta attribute="sync-DAO">false</meta>
		<composite-id name="id" class="BiBopjshRetNoPK">
			<key-property
				name="fileType"
				column="FILE_TYPE"
				type="string"
			/>
			<key-property
				name="appType"
				column="APP_TYPE"
				type="string"
			/>
		</composite-id>
		<property
			name="distCusType"
			column="DIST_CUS_TYPE"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="cusTypes"
			column="CUS_TYPES"
			type="string"
			not-null="false"
			length="512"
		/>
		<property
			name="seqMax"
			column="SEQ_MAX"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="noComb"
			column="NO_COMB"
			type="string"
			not-null="false"
			length="2"
		/>


	</class>	
</hibernate-mapping>