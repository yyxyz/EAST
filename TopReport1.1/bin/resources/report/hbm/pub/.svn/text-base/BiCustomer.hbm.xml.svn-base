<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="BiCustomer"
		table="BI_CUSTOMER"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="CUSTOMER_ID"
		>
		</id>

		<property
			name="orgId"
			column="ORG_ID"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="customerType"
			column="CUSTOMER_TYPE"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="paperType"
			column="PAPER_TYPE"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="paperTypeMemo"
			column="PAPER_TYPE_MEMO"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="paperCode"
			column="PAPER_CODE"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="customerName"
			column="CUSTOMER_NAME"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="customerEnname"
			column="CUSTOMER_ENNAME"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="customerNationality"
			column="CUSTOMER_NATIONALITY"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="telephone"
			column="TELEPHONE"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="customerAddress"
			column="CUSTOMER_ADDRESS"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="customerOthers"
			column="CUSTOMER_OTHERS"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="vocationType"
			column="VOCATION_TYPE"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="registrationMoney"
			column="REGISTRATION_MONEY"
			type="big_decimal"
			not-null="false"
			length="22"
		/>
		<property
			name="corporationRepname"
			column="CORPORATION_REPNAME"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="corporationPaperType"
			column="CORPORATION_PAPER_TYPE"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="corporationPaperId"
			column="CORPORATION_PAPER_ID"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="farenPaperTypeMemo"
			column="FAREN_PAPER_TYPE_MEMO"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="stockFlag"
			column="STOCK_FLAG"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="filler1"
			column="FILLER1"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="filler2"
			column="FILLER2"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="filler3"
			column="FILLER3"
			type="string"
			not-null="false"
			length="64"
		/>


	</class>	
</hibernate-mapping>