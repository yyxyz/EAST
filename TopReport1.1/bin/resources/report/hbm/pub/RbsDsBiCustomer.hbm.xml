<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="RbsDsBiCustomer"
		table="RBS_DS_BI_CUSTOMER"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="string"
			column="ID"
		>
		</id>

		<property
			name="branchcode"
			column="BRANCHCODE"
			type="string"
			not-null="true"
			length="3"
		/>
		<property
			name="partynumber"
			column="PARTYNUMBER"
			type="string"
			not-null="true"
			length="12"
		/>
		<property
			name="partystatus"
			column="PARTYSTATUS"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="opendate"
			column="OPENDATE"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="closedate"
			column="CLOSEDATE"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="partytype"
			column="PARTYTYPE"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="partyname"
			column="PARTYNAME"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="partynameinlocal"
			column="PARTYNAMEINLOCAL"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="nationality"
			column="NATIONALITY"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="residentialcountry"
			column="RESIDENTIALCOUNTRY"
			type="string"
			not-null="false"
			length="3"
		/>
		<property
			name="riskcountry"
			column="RISKCOUNTRY"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="idcardnumber"
			column="IDCARDNUMBER"
			type="string"
			not-null="false"
			length="28"
		/>


	</class>
</hibernate-mapping>