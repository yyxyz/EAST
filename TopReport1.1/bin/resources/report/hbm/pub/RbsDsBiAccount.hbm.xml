<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="RbsDsBiAccount"
		table="RBS_DS_BI_ACCOUNT"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="string"
			column="ID"
		>
		</id>

		<property
			name="branchcode"
			column="BRANCHCODE"
			type="string"
			not-null="true"
			length="3"
		/>
		<property
			name="accountnumber"
			column="ACCOUNTNUMBER"
			type="string"
			not-null="true"
			length="12"
		/>
		<property
			name="currencycode"
			column="CURRENCYCODE"
			type="string"
			not-null="true"
			length="3"
		/>
		<property
			name="accountstatus"
			column="ACCOUNTSTATUS"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="opendate"
			column="OPENDATE"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="closedate"
			column="CLOSEDATE"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="productcode"
			column="PRODUCTCODE"
			type="string"
			not-null="false"
			length="6"
		/>
		<property
			name="accounttype"
			column="ACCOUNTTYPE"
			type="string"
			not-null="false"
			length="6"
		/>
		<property
			name="partybranchcode"
			column="PARTYBRANCHCODE"
			type="string"
			not-null="false"
			length="3"
		/>
		<property
			name="partynumber"
			column="PARTYNUMBER"
			type="string"
			not-null="false"
			length="12"
		/>
		<property
			name="bookblanace"
			column="BOOKBLANACE"
			type="big_decimal"
			not-null="false"
			length="22"
		/>
		<property
			name="valuebalance"
			column="VALUEBALANCE"
			type="big_decimal"
			not-null="false"
			length="22"
		/>
		<property
			name="referencenumber"
			column="REFERENCENUMBER"
			type="string"
			not-null="false"
			length="4"
		/>
		<property
			name="referencename"
			column="REFERENCENAME"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="creditcardnumber"
			column="CREDITCARDNUMBER"
			type="string"
			not-null="false"
			length="28"
		/>
		<property
			name="localextention"
			column="LOCALEXTENTION"
			type="big_decimal"
			not-null="false"
			length="22"
		/>
		<property
			name="bopbranchcode"
			column="BOPBRANCHCODE"
			type="string"
			not-null="false"
			length="12"
		/>
		<property
			name="limittype"
			column="LIMITTYPE"
			type="string"
			not-null="false"
			length="12"
		/>
		<property
			name="accountstat"
			column="ACCOUNTSTAT"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="accountcata"
			column="ACCOUNTCATA"
			type="string"
			not-null="false"
			length="2"
		/>


	</class>
</hibernate-mapping>