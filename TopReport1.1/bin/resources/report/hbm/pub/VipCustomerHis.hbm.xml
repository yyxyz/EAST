<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="VipCustomerHis"
		table="vip_customer_his"
	>
		<meta attribute="sync-DAO">false</meta>
		<composite-id name="id" class="VipCustomerHisPK">
			<key-property
				name="yearMonth"
				column="year_month"
				type="string"
			/>
			<key-property
				name="cnum"
				column="cnum"
				type="string"
			/>
		</composite-id>

		<property
			name="monthVip"
			column="month_vip"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="lastMonthVip"
			column="last_month_vip"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="monthVipClass"
			column="month_vip_class"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="lastMonthClass"
			column="last_month_class"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="createdt"
			column="createdt"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="miscflg"
			column="miscflg"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="desc1"
			column="desc1"
			type="string"
			not-null="false"
			length="254"
		/>
		<property
			name="desc2"
			column="desc2"
			type="string"
			not-null="false"
			length="254"
		/>


	</class>	
</hibernate-mapping>