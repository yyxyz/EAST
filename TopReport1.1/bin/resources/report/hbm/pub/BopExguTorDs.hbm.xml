<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="BopExguTorDs"
		table="BOP_EXGU_TOR_DS"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="TOR_ID"
		>
		</id>

		<property
			name="torCode"
			column="TOR_CODE"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="torName"
			column="TOR_NAME"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="torEnname"
			column="TOR_ENNAME"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="torType"
			column="TOR_TYPE"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="countryCode"
			column="COUNTRY_CODE"
			type="string"
			not-null="false"
			length="3"
		/>
		<property
			name="crtTm"
			column="CRT_TM"
			type="java.util.Date"
			not-null="false"
			length="8"
		/>
		<property
			name="recId"
			column="REC_ID"
			type="string"
			not-null="false"
			length="32"
		/>
		
		<property
			name="torTypeCode"
			column="TOR_TYPE_CODE"
			type="string"
			not-null="false"
			length="8"
		/>

	</class>	
</hibernate-mapping>