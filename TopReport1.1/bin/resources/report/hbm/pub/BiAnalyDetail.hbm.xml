<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="BiAnalyDetail"
		table="BI_ANALY_DETAIL"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="DET_ID"
		>
		</id>

		<property
			name="analyNo"
			column="ANALY_NO"
			type="string"
			not-null="true"
			length="32"
		/>
		<property
			name="executeResult"
			column="EXECUTE_RESULT"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="confType"
			column="CONF_TYPE"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="confClassPath"
			column="CONF_CLASS_PATH"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="confIsRet"
			column="CONF_IS_RET"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="confRetClass"
			column="CONF_RET_CLASS"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="startTm"
			column="START_TM"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="endTm"
			column="END_TM"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="execSta"
			column="EXEC_STA"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="execRemark"
			column="EXEC_REMARK"
			type="string"
			not-null="false"
			length="512"
		/>
		<property
			name="confSeq"
			column="CONF_SEQ"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="confId"
			column="CONF_ID"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="confDesc"
			column="CONF_DESC"
			type="string"
			not-null="false"
			length="256"
		/>
	</class>
</hibernate-mapping>