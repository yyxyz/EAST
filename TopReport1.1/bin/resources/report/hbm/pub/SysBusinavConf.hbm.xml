<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="SysBusinavConf"
		table="SYS_BUSINAV_CONF"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="BUSI_CODE"
		>
		</id>

		<property
			name="busiNm"
			column="BUSI_NM"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="parentCode"
			column="PARENT_CODE"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="showSeq"
			column="SHOW_SEQ"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="urlPath"
			column="URL_PATH"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="approveUrl"
			column="APPROVE_URL"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="queryUrl"
			column="QUERY_URL"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="makefileUrl"
			column="MAKEFILE_URL"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="busiType"
			column="BUSI_TYPE"
			type="string"
			not-null="false"
			length="2"
		/>

	</class>
</hibernate-mapping>