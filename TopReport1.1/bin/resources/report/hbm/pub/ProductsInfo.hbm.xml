<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="ProductsInfo"
		table="products_info"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="pid"
		>
		</id>

		<property
			name="pname"
			column="pname"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="termType"
			column="term_type"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="term"
			column="term"
			type="string"
			not-null="false"
			length="6"
		/>
		<property
			name="lowLimit"
			column="low_limit"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="highLimit"
			column="high_limit"
			type="big_decimal"
			not-null="false"
			length="20"
		/>
		<property
			name="status"
			column="status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="effectDate"
			column="effect_date"
			type="date"
			not-null="false"
			length="23"
		/>
		<property
			name="expireDate"
			column="expire_date"
			type="date"
			not-null="false"
			length="23"
		/>
		<property
			name="lastUpdt"
			column="last_updt"
			type="date"
			not-null="false"
			length="23"
		/>
		<property
			name="miscflg"
			column="miscflg"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="desc1"
			column="desc1"
			type="string"
			not-null="false"
			length="254"
		/>
		<property
			name="desc2"
			column="desc2"
			type="string"
			not-null="false"
			length="254"
		/>


	</class>	
</hibernate-mapping>