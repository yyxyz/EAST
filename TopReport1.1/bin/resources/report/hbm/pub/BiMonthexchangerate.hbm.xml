<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="BiMonthexchangerate"
		table="BI_MONTHEXCHANGERATE"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="RATE_CUR"
		>
		</id>

		<property
			name="rateUnit"
			column="RATE_UNIT"
			type="big_decimal"
			not-null="false"
			length="9"
		/>
		<property
			name="yearMonth"
			column="YEAR_MONTH"
			type="string"
			not-null="false"
			length="6"
		/>
		<property
			name="rateMidprice"
			column="RATE_MIDPRICE"
			type="big_decimal"
			not-null="false"
			length="16"
		/>
		<property
			name="filler1"
			column="FILLER1"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="filler2"
			column="FILLER2"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="filler3"
			column="FILLER3"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="st"
			column="ST"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="lock"
			column="IS_LOCK"
			type="true_false"
			not-null="false"
			length="1"
		/>
		<property
			name="del"
			column="IS_DEL"
			type="true_false"
			not-null="false"
			length="1"
		/>
		<property
			name="crtDt"
			column="CRT_DT"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="lastUpdOper"
			column="LAST_UPD_OPER"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="lastUpdTms"
			column="LAST_UPD_TMS"
			type="string"
			not-null="false"
			length="14"
		/>

	</class>
</hibernate-mapping>