<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="SubFileConf"
		table="SUB_FILE_CONF"
	>
		<meta attribute="sync-DAO">false</meta>
		<composite-id name="id" class="SubFileConfPK">
			<key-property
				name="fileType"
				column="FILE_TYPE"
				type="string"
			/>
			<key-property
				name="busiType"
				column="BUSI_TYPE"
				type="string"
			/>
			<key-property
				name="appType"
				column="APP_TYPE"
				type="string"
			/>
		</composite-id>
		<property
			name="xmlFileId"
			column="XML_FILE_ID"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="fileResultPath"
			column="FILE_RESULT_PATH"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="execSeq"
			column="EXEC_SEQ"
			type="integer"
			not-null="false"
		/>
		<property
			name="busiPkStr"
			column="BUSI_PK_STR"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="pkSplit"
			column="PK_SPLIT"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="dataValidPath"
			column="DATA_VALID_PATH"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="confIsControl"
			column="CONF_IS_CONTROL"
			type="string"
			not-null="false"
			length="1"
		/>
	</class>
</hibernate-mapping>