<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.report">
	<class
		name="MtsBopCustom"
		table="MTS_BOP_CUSTOM"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="ID"
		>
		</id>

		<property
			name="recId"
			column="REC_ID"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="crtTm"
			column="CRT_TM"
			type="java.util.Date"
			not-null="false"
			length="34"
		/>
		<property
			name="customn"
			column="CUSTOMN"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="custccy"
			column="CUSTCCY"
			type="string"
			not-null="false"
			length="3"
		/>
		<property
			name="custamt"
			column="CUSTAMT"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="offamt"
			column="OFFAMT"
			type="integer"
			not-null="false"
			length="22"
		/>


	</class>	
</hibernate-mapping>