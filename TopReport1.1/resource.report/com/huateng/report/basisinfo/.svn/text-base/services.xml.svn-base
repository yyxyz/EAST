<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "/WEB-INF/dtd/spring-beans-2.0.dtd">
<beans>
    <!-- 日牌价服务类 -->
    <bean id="BiDayExchangeRateService"
		class="com.huateng.report.basis.service.BiDayExchangeRateService">
	</bean>
	<!-- 月牌价服务类 -->
	<bean id="BiMonthExchangeRateService"
		class="com.huateng.report.basis.service.BiMonthExchangeRateService">
	</bean>
	<!-- 日牌价操作-->
    <bean id="BiDayExchangeRateOperation"
       class="com.huateng.report.basis.operation.BiDayExchangeRateOperation">
    </bean>
    <!-- 月牌价操作 -->
    <bean id="BiMonthExchangeRateOperation"
       class="com.huateng.report.basis.operation.BiMonthExchangeRateOperation">
    </bean>

	<!-- 币种信息维护 -->
    <bean id="CurrencyManOperation"
       class="com.huateng.report.basis.operation.CurrencyManOperation">
    </bean>
    <!-- 客户账户信息维护 -->
    <bean id="BiAccountOperation"
       class="com.huateng.report.basis.operation.BiAccountOperation">
    </bean>
	<!-- 国家/地区维护service -->
	<bean id="biNationregionService"
			class="com.huateng.report.basis.service.BiNationregionService" />
	<!-- 国家/地区维护Operation -->
	<bean id="biNationregionOperation"
			class="com.huateng.report.basis.operation.BiNationregionOperation"/>

	<!-- 客户信息管理 -->
   <bean id="CustomerReaOperation" 
   		class="com.huateng.report.basis.operation.CustomerReaOperation">
   	</bean>
</beans>
