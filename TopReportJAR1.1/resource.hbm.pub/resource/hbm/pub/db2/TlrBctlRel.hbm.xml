<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.pub">
	<class
		name="TlrBctlRel"
		table="TLR_BCTL_REL"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="ID"
		>
			<generator class="assigned">
				 
			</generator>
		</id>

		<property
			name="tlrNo"
			column="TLR_NO"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="brcode"
			column="BRCODE"
			type="string"
			not-null="false"
			length="20"
		/>


	</class>	
</hibernate-mapping>