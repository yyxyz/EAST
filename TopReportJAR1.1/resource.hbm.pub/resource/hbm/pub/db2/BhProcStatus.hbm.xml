<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.pub">
	<class
		name="BhProcStatus"
		table="BH_PROC_STATUS"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="ID"
		>
			<generator class="native">
				 
			</generator>
		</id>

		<property
			name="bhdate"
			column="BHDATE"
			type="date"
			not-null="true"
			length="7"
		/>
		<property
			name="jobno"
			column="JOBNO"
			type="integer"
			not-null="true"
			length="22"
		/>
		<property
			name="step"
			column="STEP"
			type="integer"
			not-null="true"
			length="22"
		/>
		<property
			name="subStep"
			column="SUB_STEP"
			type="integer"
			not-null="true"
			length="22"
		/>
		<property
			name="processFunction"
			column="PROCESS_FUNCTION"
			type="string"
			not-null="false"
			length="120"
		/>
		<property
			name="processid"
			column="PROCESSID"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="startTime"
			column="START_TIME"
			type="java.sql.Timestamp"
			not-null="false"
		/>
		<property
			name="endTime"
			column="END_TIME"
			type="java.sql.Timestamp"
			not-null="false"
		/>
		<property
			name="subFlag"
			column="SUB_FLAG"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="status"
			column="STATUS"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="branchlist"
			column="BRANCHLIST"
			type="string"
			not-null="false"
			length="1024"
		/>
		<property
			name="desc1"
			column="DESC1"
			type="string"
			not-null="false"
			length="1024"
		/>
		<property
			name="desc2"
			column="DESC2"
			type="string"
			not-null="false"
			length="1024"
		/>
		<property
			name="timestamps"
			column="TIMESTAMPS"
			type="date"
			not-null="false"
		/>


	</class>
</hibernate-mapping>