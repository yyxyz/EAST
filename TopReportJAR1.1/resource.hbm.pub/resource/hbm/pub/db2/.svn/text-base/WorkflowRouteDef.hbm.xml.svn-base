<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.huateng.ebank.entity.data.workflow">
	<class
		name="WorkflowRouteDef"
		table="WORKFLOW_ROUTE_DEF"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="ROUTE_ID"
		>
			 <generator class="native" >
            	 
            </generator>

		</id>
		<property
			name="routeName"
			column="ROUTE_NAME"
			type="string"
			not-null="false"
			length="512"
		/>
		<property
			name="brhClass"
			column="BRH_CLASS"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="isBand"
			column="IS_BAND"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="status"
			column="STATUS"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="isSet"
			column="IS_SET"
			type="string"
			not-null="false"
			length="1"
		/>


	</class>
</hibernate-mapping>