<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.huateng.ebank.entity.data.workflow">
	<class
		name="BrhWorkflowDef"
		table="BRH_WORKFLOW_DEF"
	>
		<meta attribute="sync-DAO">false</meta>

		<id
			name="id"
			type="long"
			column="ID"
		>
			<generator class="native">
				 
			</generator>
		</id>
		<property
			name="brcode"
			column="BRCODE"
			type="string"
			not-null="true"
			length="4"
		/>
		<property
			name="apptype"
			column="APPTYPE"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="bizClass"
			column="BIZ_CLASS"
			type="string"
			not-null="false"
			length="6"
		/>
		<property
			name="bizSubclass"
			column="BIZ_SUBCLASS"
			type="string"
			not-null="true"
			length="6"
		/>
		<property
			name="processTemplate"
			column="PROCESS_TEMPLATE"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="effectDate"
			column="EFFECT_DATE"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="expireDate"
			column="EXPIRE_DATE"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="status"
			column="STATUS"
			type="string"
			not-null="false"
			length="1"
		/>


	</class>	
</hibernate-mapping>