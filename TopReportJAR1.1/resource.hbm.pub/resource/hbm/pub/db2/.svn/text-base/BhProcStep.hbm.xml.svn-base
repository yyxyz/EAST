<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.pub">
	<class
		name="BhProcStep"
		table="BH_PROC_STEP"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="ID"
		>
			<generator class="native">
				 
			</generator>
		</id>

		<property
			name="jobno"
			column="JOBNO"
			type="integer"
			not-null="true"
			length="22"
		/>
		<property
			name="step"
			column="STEP"
			type="integer"
			not-null="true"
			length="22"
		/>
		<property
			name="subStep"
			column="SUB_STEP"
			type="integer"
			not-null="true"
			length="22"
		/>
		<property
			name="processFunction"
			column="PROCESS_FUNCTION"
			type="string"
			not-null="true"
			length="120"
		/>
		<property
			name="processParam"
			column="PROCESS_PARAM"
			type="string"
			not-null="false"
			length="512"
		/>
		<property
			name="processTlrno"
			column="PROCESS_TLRNO"
			type="string"
			not-null="false"
			length="7"
		/>
		<property
			name="runtime"
			column="RUNTIME"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="subFlag"
			column="SUB_FLAG"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="maxproc"
			column="MAXPROC"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="tempFlag"
			column="TEMP_FLAG"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="suspend"
			column="SUSPEND"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="auto"
			column="AUTO"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="timestamps"
			column="TIMESTAMPS"
			type="date"
			not-null="false"
			length="11"
		/>
		<property
			name="desc0"
			column="DESC0"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="desc1"
			column="DESC1"
			type="string"
			not-null="false"
			length="1024"
		/>
		<property
			name="desc2"
			column="DESC2"
			type="string"
			not-null="false"
			length="1024"
		/>
		<property
			name="reportFlag"
			column="REPORT_FLAG"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="reportDataFlag"
			column="REPORT_DATA_FLAG"
			type="string"
			not-null="false"
			length="1"
		/>


	</class>
</hibernate-mapping>