<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.huateng.ebank.entity.data.workflow">
	<class
		name="LimitParam"
		table="LIMIT_PARAM"
		lazy="false"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="long"
			column="ID"
		>
			<generator class="native">
					 
			</generator>
		</id>

		<property
			name="tlrno"
			column="TLRNO"
			type="string"
			not-null="true"
		/>
		<property
			name="bizClass"
			column="BIZ_CLASS"
			type="string"
			not-null="true"
		/>
		<property
			name="bizSubclass"
			column="BIZ_SUBCLASS"
			type="string"
			not-null="true"
		/>
		<property
			name="limitMode"
			column="LIMIT_MODE"
			type="string"
			not-null="false"
		/>
		<property
			name="limitMinamount"
			column="LIMIT_MINAMOUNT"
			type="big_decimal"
			not-null="false"
			length="16"
		/>
		<property
			name="limitMaxamount"
			column="LIMIT_MAXAMOUNT"
			type="big_decimal"
			not-null="false"
			length="16"
		/>
		<property
			name="precondition"
			column="PRECONDITION"
			type="string"
			not-null="false"
		/>
		<property
			name="status"
			column="STATUS"
			type="string"
			not-null="false"
		/>
		<property
			name="effectDate"
			column="EFFECT_DATE"
			type="date"
			not-null="false"
		/>
		<property
			name="expireDate"
			column="EXPIRE_DATE"
			type="date"
			not-null="false"
		/>
		<property
			name="miscflgs"
			column="MISCFLGS"
			type="string"
			not-null="false"
		/>
		<property
			name="misc"
			column="MISC"
			type="string"
			not-null="false"
		/>
		<property
			name="lastUpdDate"
			column="LAST_UPD_DATE"
			type="date"
			not-null="false"
		/>
		<property
			name="lastUpdFunc"
			column="LAST_UPD_FUNC"
			type="string"
			not-null="false"
		/>
		<property
			name="lastUpdTlr"
			column="LAST_UPD_TLR"
			type="string"
			not-null="false"
		/>
		<property
			name="timestamps"
			column="TIMESTAMPS"
			type="timestamp"
			not-null="false"
			length="26"
		/>


	</class>
</hibernate-mapping>