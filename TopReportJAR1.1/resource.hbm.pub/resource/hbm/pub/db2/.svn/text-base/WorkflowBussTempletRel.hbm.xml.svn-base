<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.huateng.ebank.entity.data.workflow">
	<class
		name="WorkflowBussTempletRel"
		table="WORKFLOW_BUSS_TEMPLET_REL"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="ID"
		>
			<generator class="native" >
            	 
            </generator>

		</id>

		<property
			name="templetId"
			column="TEMPLET_ID"
			type="integer"
			length="10"
		/>

		<property
			name="bussProc"
			column="BUSS_PROC"
			type="string"
			length="100"
		/>

		<property
			name="description"
			column="DESCRIPTION"
			type="string"
			length="100"
		/>

		<property
			name="templetName"
			column="TEMPLET_NAME"
			type="string"
			length="100"
		/>

		<property
			name="templetType"
			column="TEMPLET_TYPE"
			type="string"
			length="2"
		/>

		<property
			name="status"
			column="STATUS"
			type="string"
			length="1"
		/>

		<property
			name="templetDesc"
			column="TEMPLET_DESC"
			type="string"
			length="100"
		/>

	</class>
</hibernate-mapping>