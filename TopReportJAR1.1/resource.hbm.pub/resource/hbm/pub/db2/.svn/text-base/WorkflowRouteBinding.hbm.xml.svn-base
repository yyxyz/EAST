<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.huateng.ebank.entity.data.workflow">
	<class
		name="WorkflowRouteBinding"
		table="WORKFLOW_ROUTE_BINDING"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="ID"
		>
			<generator class="native" >
            	 
            </generator>
		</id>

		<property
			name="bussType"
			column="BUSS_TYPE"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="draftType"
			column="DRAFT_TYPE"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="maxAmt"
			column="MAX_AMT"
			type="big_decimal"
			not-null="false"
			length="15"
		/>
		<property
			name="startBrhid"
			column="START_BRHID"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="startBrhno"
			column="START_BRHNO"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="brhClass"
			column="BRH_CLASS"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="routeId"
			column="ROUTE_ID"
			type="integer"
			not-null="false"
			length="10"
		/>


	</class>
</hibernate-mapping>