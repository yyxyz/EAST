			length="1"
		/>
		<property
			name="bizClass"
			column="BIZ_CLASS"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="bizSubclass"
			column="BIZ_SUBCLASS"
			type="string"
			not-null="true"
			length="3"
		/>
		<property
			name="brcodeType"
			column="BRCODE_TYPE"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="brcodeList"
			column="BRCODE_LIST"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="assignType"
			column="ASSIGN_TYPE"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
