			column="PASS"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="miscflgs"
			column="MISCFLGS"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="misc"
			column="MISC"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="createTlr"
			column="CREATE_TLR"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="createDate"
			column="CREATE_DATE"
			type="date"
			not-null="false"
			length="10"
		/>
