			name="lastUpdTlr"
			column="LAST_UPD_TLR"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="timestamps"
			column="TIMESTAMPS"
			type="timestamp"
			not-null="false"
			length="26"
		/>

		<property
			name="limitation"
			column="LIMITATION"
			type="integer"
			not-null="false"
		/>


	</class>
</hibernate-mapping>