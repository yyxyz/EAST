<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.huateng.ebank.entity.data.workflow">
	<class
		name="WorkflowParam"
		table="WORKFLOW_PARAM"
		lazy="false"
	>
		<meta attribute="sync-DAO">false</meta>

		<id
			name="id"
			type="long"
			column="ID"
		>
			<generator class="native">
				 
			</generator>
		</id>

		<property
			name="processTemplate"
			column="PROCESS_TEMPLATE"
			type="string"
			not-null="true"
			length="100"
		/>
		<property
			name="taskName"
			column="TASK_NAME"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="apptype"
			column="APPTYPE"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
