<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.pub">
	<class
		name="RoleInfo"
		table="ROLE_INFO"
		lazy="false"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="ROLE_ID"
		>
 			<generator class="assigned" />

		</id>

		<property
			name="roleName"
			column="ROLE_NAME"
			type="string"
			not-null="false"
		/>
		<property
			name="status"
			column="STATUS"
			type="string"
			not-null="false"
		/>
		<property
			name="effectDate"
			column="EFFECT_DATE"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="expireDate"
			column="EXPIRE_DATE"
			type="date"
			not-null="false"
			length="7"
		/>
		<!--
		<property
			name="workflowRole"
			column="WORKFLOW_ROLE"
			type="string"
			not-null="false"
		/>
		-->

		<property
			name="roleType"
			column="ROLE_TYPE"
			type="string"
			not-null="false"
		/>
		<!--
		<property
			name="flag"
			column="FLAG"
			type="string"
			not-null="false"
		/>
		-->

		<property
			name="brclass"
			column="BRCLASS"
			type="string"
			not-null="false"
		/>

		<property
			name="lastUpdDate"
			column="LAST_UPD_DATE"
			type="date"
			not-null="false"
			length="7"
		/>
		<property
			name="lastUpdFunc"
			column="LAST_UPD_FUNC"
			type="string"
			not-null="false"
		/>
		<property
			name="lastUpdTlr"
			column="LAST_UPD_TLR"
			type="string"
			not-null="false"
		/>
		<property
			name="timestamps"
			column="TIMESTAMPS"
			type="date"
			not-null="false"
			length="11"
		/>
		<property
			name="miscflgs"
			column="MISCFLGS"
			type="string"
			not-null="false"
		/>
		<property
			name="misc"
			column="MISC"
			type="string"
			not-null="false"
		/>
		<!-- add by zhiyang.he 修改锁定状态 2012-09-6 begin-->
		<property
			name="isLock"
			column="IS_LOCK"
			type="string"
			length="1"
			not-null="false"
		/>
		<property
			name="st"
			column="ST"
			type="string"
			length="1"
			not-null="false"
		/>
		<property
			name="crtDt"
			column="CRT_DT"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="lastUpdOper"
			column="LAST_UPD_OPER"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="lastUpdTms"
			column="LAST_UPD_TMS"
			type="string"
			not-null="false"
			length="14"
		/>
		<!-- add by zhiyang.he 修改锁定状态 2012-09-6 end-->

	</class>
</hibernate-mapping>