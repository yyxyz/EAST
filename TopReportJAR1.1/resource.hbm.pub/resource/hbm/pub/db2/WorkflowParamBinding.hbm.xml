<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.huateng.ebank.entity.data.workflow">
	<class
		name="WorkflowParamBinding"
		table="WORKFLOW_PARAM_BINDING"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="long"
			column="ID"
		>
			<generator class="native">
				 
			</generator>
		</id>

		<property
			name="processTemplate"
			column="PROCESS_TEMPLATE"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="taskName"
			column="TASK_NAME"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="apptype"
			column="APPTYPE"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="bizClass"
			column="BIZ_CLASS"
			type="string"
			not-null="false"
			length="4"
		/>
		<property
			name="bizSubclass"
			column="BIZ_SUBCLASS"
			type="string"
			not-null="false"
			length="4"
		/>
		<property
			name="maxAmt"
			column="MAX_AMT"
			type="big_decimal"
			not-null="false"
			length="16"
		/>
		<property
			name="startBrhid"
			column="START_BRHID"
			type="string"
			not-null="false"
			length="6"
		/>
		<property
			name="brhClass"
			column="BRH_CLASS"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="roleId"
			column="ROLE_ID"
			type="integer"
			not-null="false"
			length="22"
		/>


	</class>	
</hibernate-mapping>